/* pages/information/information.wxss */
page{
  background: #f6f6f6;
}

.information{
  width: 100%;
  height: 201rpx;
  background: #fff;
  margin-top: 20rpx;
}

.information-li {
  width: 96%;
  height: 120rpx;
  border-bottom: 1rpx solid #f6f6f6;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-left: 2%;
  font-size: 30rpx;
}

.information-img{
  width: 150rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.centerImg {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
}